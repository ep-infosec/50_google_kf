{
    "sourcePackage": {},
    "name": "buildpackv2",
    "kind": "KfBuiltinTask",
    "apiVersion": "builtin.kf.dev/v1alpha1",
    "params": [
        {
            "name": "SOURCE_IMAGE",
            "value": "some/source/image"
        },
        {
            "name": "BUILDPACKS",
            "value": "buildpack-1"
        },
        {
